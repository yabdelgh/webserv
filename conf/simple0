
   
server {
	listen 0.0.0.0:8000

	root /Users/aait-ihi/Desktop/webserv/public/www
	server_name weebserv

	index hello.html index.html

	autoindex on
	client_body_buffer_size 10
	
	error_page 404 403 error.html

	location / {
		client_body_buffer_size 20k
		index index.html
		root test_us/root
		autoindex on
	}
}
